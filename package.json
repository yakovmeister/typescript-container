{
  "name": "typescript-container",
  "version": "0.0.0",
  "description": "Laravel (illuminate/container) inspired/based IoC container.",
  "main": "dist/index.min.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**.test.ts",
    "rollup": "rollup -c",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run rollup && npm run transpile && npm run minify",
    "transpile": "babel dist/index.js --out-file dist/index.js --presets=@babel/preset-env,@babel/preset-stage-3",
    "minify": "uglifyjs dist/index.js --output dist/index.min.js"
  },
  "keywords": ["ioc", "laravel", "angular", "container", "inversion of control", "laravel based", "broken"],
  "author": "Jacob Baring <so@tfwno.gf>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-stage-3": "^7.0.0-beta.47",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.0",
    "babel-compile": "^2.0.0",
    "babel-minify": "^0.4.3",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.25"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.47"
  }
}
